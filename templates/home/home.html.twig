			<script src="{{ asset('./web/js/jquery.min.js') }}"></script><!-- jQuery Easing --><script src="{{ asset('./web/js/jquery.easing.1.3.js') }}"></script><!-- Bootstrap --><script src="./web/js/bootstrap.min.js"></script><!-- Waypoints --><script src="{{ asset('./web/js/jquery.waypoints.min.js') }}"></script><!-- Flexslider --><script src="{{ asset('./web/js/jquery.flexslider-min.js') }}"></script><!-- Sticky Kit --><script src="{{ asset('./web/js/sticky-kit.min.js') }}"></script><!-- Owl carousel --><script src="{{ asset('./web/js/owl.carousel.min.js') }}"></script><!-- Counters --><script src="{{ asset('./web/js/jquery.countTo.js') }}"></script><!-- MAIN JS --><script src="{{ asset('./web/js/main.js') }}"></script>
